{
  "name": "better-convex",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "next build",
    "predev": "convex dev --until-success --run init",
    "dev": "concurrently 'pnpm dev:app' 'pnpm dev:backend'",
    "dev:app": "next dev --turbopack --port 3005",
    "dev:backend": "convex dev",
    "gen": "cd convex/betterAuth && npx @better-auth/cli generate -y --output generatedSchema.ts",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix && prettier --write . --log-level warn",
    "nuke": "rimraf '**/node_modules'",
    "reset": "convex run reset:reset && sleep 3 && convex run init",
    "reset:auth": "convex run reset:resetAuth",
    "seed": "convex run seed:seed",
    "start": "next start",
    "studio": "pnpm convex dashboard",
    "sync": "tsx tooling/scripts/sync-convex-env.ts",
    "sync:force": "tsx tooling/scripts/sync-convex-env.ts --force",
    "sync:plate": "node tooling/scripts/sync-plate.cjs",
    "typecheck": "tsc --noEmit",
    "typecheck:watch": "pnpm typecheck --watch"
  },
